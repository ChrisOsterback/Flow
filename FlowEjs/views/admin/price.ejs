            <th>title</th>
            <th>price</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <% prices.forEach(function(price){ %>
        <tr>
            <td><%= price.first %></td>
            <td><%= price.second %></td>
            

               
            </td>
            <td><a href="/admin/price/edit-price/<%= price._id %>">Edit</a></td>
            <td><a class="confirmDeletion" href="/admin/price/delete-price/<%= price._id %>">Delete</a></td>
        </tr>
        <% }); %>
    </tbody>
</table>

