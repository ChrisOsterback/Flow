<%- include('../_layouts/adminheader/adminheader') %>

<h2 class="page-title">PriceList</h2>






<br><br>
<h1>HAIR CUT PRICE</h1>
<a href="/admin/price/add-klipp" class="btn btn-primary">Add hair cut content</a>
<table class="table table-striped alignmiddle">
    <thead>
        <tr class="home">
            <th>price</th>
            <th>price</th>
            <th>price</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <% klipps.forEach(function(klipps){ %>
        <tr>
            <td><%= klipps.first %></td>
            <td><%= klipps.second %></td>
            <td><%= klipps.third %></td>  
            </td>
            <td><a href="/admin/price/edit-klipp/<%= klipps._id %>">Edit</a></td>
            <td><a class="confirmDeletion" href="/admin/price/delete-klipp/<%= klipps._id %>">Delete</a></td>
        </tr>
        <% }); %>
    </tbody>
</table>

<br><br><br><br>

<h1>PACKAGE PRICE</h1>
<a href="/admin/price/add-price" class="btn btn-primary">Add new package content</a>

